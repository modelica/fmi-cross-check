&#xd;
    &lt;!-- Convert distortion widgets to assignment widgets --&gt;&#xd;
    &lt;xsl:template match=&quot;/workspace/widgets/widget-storage-container/widget[@class=&apos;distortion-widget&apos;]&quot;&gt;&#xd;
        &lt;widget class=&quot;assignment-widget&quot;&gt;&#xd;
            &lt;expressionColumnWidth&gt;100&lt;/expressionColumnWidth&gt;&#xd;
            &lt;commentColumnWidth&gt;100&lt;/commentColumnWidth&gt;&#xd;
            &lt;expressions&gt;&#xd;
                &lt;xsl:variable name=&quot;factor&quot; select=&quot;modifier/factor&quot;/&gt;&#xd;
                &lt;xsl:variable name=&quot;offset&quot; select=&quot;modifier/offset&quot;/&gt;&#xd;
                &lt;xsl:variable name=&quot;sinusM&quot; select=&quot;modifier/sinusM&quot;/&gt;&#xd;
                &lt;xsl:variable name=&quot;sinusP&quot; select=&quot;modifier/sinusP&quot;/&gt;&#xd;
                &lt;xsl:variable name=&quot;noise&quot; select=&quot;modifier/noise&quot;/&gt;&#xd;
                &lt;xsl:variable name=&quot;delay&quot; select=&quot;modifier/delay&quot;/&gt;&#xd;
                &lt;xsl:for-each select=&quot;../data/modify-variables-list/list/widget-variable/name&quot;&gt;&#xd;
                    &lt;xsl:variable name=&quot;var&quot; select=&quot;.&quot;/&gt;&#xd;
                    &lt;assignment&gt;&#xd;
                        &lt;expression&gt;&#xd;
                            &lt;xsl:value-of&#xd;
                                    select=&quot;concat($var, &apos;= (delay(&apos;, $var, &apos;,&apos;, $delay,&apos;/1000)&apos;,&apos; * &apos;,$factor,&apos; + &apos;, $offset,&apos; + sin(time / &apos;,$sinusP,&apos; * (2 * 3.1415676535897985)) * &apos;, $sinusM,&apos; + (2*random(32767)/32767 - 1) * &apos;,$noise,&apos;)&apos;)&quot;/&gt;&#xd;
                        &lt;/expression&gt;&#xd;
                        &lt;enabled&gt;true&lt;/enabled&gt;&#xd;
                    &lt;/assignment&gt;&#xd;
                &lt;/xsl:for-each&gt;&#xd;
                &lt;assignment&gt;&#xd;
                    &lt;expression/&gt;&#xd;
                    &lt;enabled&gt;true&lt;/enabled&gt;&#xd;
                &lt;/assignment&gt;&#xd;
            &lt;/expressions&gt;&#xd;
        &lt;/widget&gt;&#xd;
    &lt;/xsl:template&gt;&#xd;
&#xd;
    &lt;!-- Set realTime mode to true if restrictRealCan is false --&gt;&#xd;
    &lt;xsl:template match=&quot;/workspace/preferences/properties/property[@name = &apos;restrictRealCan&apos; and @value = &apos;false&apos;]&quot;&gt;&#xd;
        &lt;property name=&quot;realTime&quot; value=&quot;true&quot;/&gt;&#xd;
    &lt;/xsl:template&gt;&#xd;
&lt;/xsl:stylesheet&gt;</xslt>
      <message>Added CHECK mode to button widgets, ignore list to diff widget, enabled condition to modules</message>

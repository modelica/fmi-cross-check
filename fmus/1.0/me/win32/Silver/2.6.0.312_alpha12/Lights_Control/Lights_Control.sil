            &lt;/list&gt;&#xd;
        &lt;/entry&gt;&#xd;
    &lt;/xsl:template&gt;&#xd;
&#xd;
    &lt;!-- Change value-table to List&lt;Pair&lt;String, Double&gt; in data plotter widget --&gt;&#xd;
    &lt;xsl:template&#xd;
            match=&quot;/workspace/widgets/widget-storage-container[./widgetClass/text() = &apos;data-plotter-widget&apos;]/data/read-variables-list/list/widget-variable/properties/entry[./string/text()=&apos;valueTable&apos;]/value-table&quot;&gt;&#xd;
        &lt;list&gt;&#xd;
            &lt;xsl:for-each select=&quot;value-table-row&quot;&gt;&#xd;
                &lt;pair&gt;&#xd;
                    &lt;first class=&quot;string&quot;&gt;&#xd;
                        &lt;xsl:value-of select=&quot;label&quot;/&gt;&#xd;
                    &lt;/first&gt;&#xd;
                    &lt;second class=&quot;double&quot;&gt;&#xd;
                        &lt;xsl:value-of select=&quot;value&quot;/&gt;&#xd;
                    &lt;/second&gt;&#xd;
